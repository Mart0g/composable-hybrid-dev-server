{
  "root": true,
  "extends": "airbnb-base/legacy",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "node": true
  },
  "rules": {
    "max-len": "off",
    "no-console": "off",
    "no-new-func": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "no-useless-escape": "off"
  }
}
